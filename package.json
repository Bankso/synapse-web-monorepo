{
  "name": "synapse-react-client",
  "version": "1.5.2",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Sage-Bionetworks/Synapse-React-Client"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "bootswatch": "git+https://git@github.com/thomaspark/bootswatch.git#95a134434fd0955182e9a938b5e50624e8e143c6",
    "i": "^0.3.6",
    "katex": "^0.10.0-rc.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "markdown-it": "^8.4.2",
    "markdown-it-br": "^1.0.0",
    "markdown-it-center-text": "^1.0.4",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emphasis-alt": "^1.0.8",
    "markdown-it-inline-comments": "^1.0.1",
    "markdown-it-strikethrough-alt": "^1.0.0",
    "markdown-it-sub-alt": "^1.0.0",
    "markdown-it-sup-alt": "^1.0.2",
    "markdown-it-synapse": "^1.1.1",
    "markdown-it-synapse-heading": "^1.0.1",
    "markdown-it-synapse-math": "^3.0.4",
    "markdown-it-synapse-table": "^1.0.6",
    "node-sass-chokidar": "^1.3.3",
    "npm": "^6.4.1",
    "npm-run-all": "^4.1.3",
    "plotly.js": "^1.40.0",
    "prop-types": "^15.6.2",
    "react-measure": "^2.1.2",
    "react-plotly.js": "^2.2.0",
    "react-test-renderer": "^16.4.2",
    "sanitize-html": "^1.18.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "^1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "NODE_PATH=./src react-scripts start",
    "start": "NODE_PATH=./src npm-run-all -p watch-css start-js",
    "build-js": "NODE_PATH=./src rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "build": "NODE_PATH=./src npm-run-all build-css build-js",
    "test": "NODE_PATH=./src react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts eject"
  }
}
