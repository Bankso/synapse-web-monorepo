  "scripts": {
    "prepublish": "npm run build",
    "predeploy": "npm run build-gh",
    "build-gh": "react-scripts-ts build",
    "deploy": "gh-pages -d build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts-ts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts-ts eject",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build-css && npm run build:types && npm run build:js",
    "build:types": "tsc ",
    "build:js": "rm -rf dist && babel src/lib --copy-files --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  }
