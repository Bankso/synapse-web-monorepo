{
  "name": "markdown-it-synapse",
  "version": "1.1.11",
  "description": "tag for markdown-it markdown parser.",
  "main": "dist/index.js",
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "synapse"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsup ./src/index.ts --format esm,cjs --dts",
    "predist": "rm -rf dist && pnpm build && pnpm test",
    "dist": "pnpm browserify && pnpm minify",
    "lint": "eslint src/*",
    "prepublish": "pnpm dist",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "homepage": "https://github.com/Sage-Bionetworks/markdown-it-synapse",
  "repository": {
    "type": "git",
    "url": "git://github.com/Sage-Bionetworks/markdown-it-synapse.git"
  },
  "bugs": {
    "url": "https://github.com/Sage-Bionetworks/markdown-it-synapse/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.16.12",
    "jest": "^29.4.3",
    "markdown-it": "^13.0.1",
    "markdown-it-testgen": "~0.1.0",
    "tsup": "^6.7.0",
    "typescript": "5.0.4"
  },
  "directories": {
    "test": "test"
  },
  "author": "Jay"
}
